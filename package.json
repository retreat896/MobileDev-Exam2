{
  "name": "mobiledev-exam2",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "patch-package",
    "build-apk": "set EAS_SKIP_AUTO_FINGERPRINT=1 && eas build --platform android --profile development",
    "build-gradle": "npx expo prebuild && cd android && (IF EXIST \\app\\build rmdir /S /Q \\app\\build) && (IF EXIST .gradle rmdir /S /Q .gradle) && gradlew clean --stacktrace",
    "start": "npx expo prebuild --clean && expo start",
    "update-dependencies": "npm run reinstall-packages && npm run build-gradle && npm run start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "check-packages": "npx expo-doctor --verbose && npx expo install --check",
    "install-packages": "npm install && npm run postinstall",
    "reinstall-packages": "(IF EXIST node_modules rmdir /S /Q .\\node_modules) && npm run install-packages"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.6",
    "@shopify/react-native-skia": "v2.0.0-next.4",
    "expo": "~53.0.0",
    "expo-build-properties": "~0.14.8",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.1",
    "expo-linking": "~7.1.7",
    "expo-media-library": "~17.1.7",
    "expo-router": "~5.1.7",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-color-picker": "^0.6.0",
    "react-native-fast-opencv": "^0.4.7",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-vision-camera": "4.7.3",
    "react-native-web": "^0.20.0",
    "react-native-worklets-core": "1.6.2",
    "vision-camera-resize-plugin": "^3.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
