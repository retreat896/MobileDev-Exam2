{
  "name": "mobiledev-exam2",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "patch-package",
    "build-apk": "set EAS_SKIP_AUTO_FINGERPRINT=1 && eas build --platform android --profile development",
    "build-gradle": "npx expo prebuild --clean && cd android && (IF EXIST \\app\\build rmdir /S /Q \\app\\build) && (IF EXIST .gradle rmdir /S /Q .gradle) && gradlew clean --stacktrace",
    "start": "expo start --tunnel",
    "update-dependencies": "npm run reinstall-packages && npm run build-gradle && npm run start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "check-packages": "npx expo-doctor --verbose && npx expo install --check",
    "install-packages": "npm install && npm run postinstall",
    "reinstall-packages": "(IF EXIST node_modules rmdir /S /Q .\\node_modules) && npm run install-packages"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@shopify/react-native-skia": "2.1.1",
    "@types/react": "^19.1.0",
    "color-convert": "^3.1.3",
    "expo": "~54.0.23",
    "expo-build-properties": "~1.0.9",
    "expo-camera": "~17.0.9",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.18",
    "expo-file-system": "^19.0.19",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.8",
    "expo-media-library": "~18.2.0",
    "expo-router": "~6.0.14",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-color-picker": "^0.6.0",
    "react-native-fast-opencv": "^0.4.7",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-vision-camera": "4.7.3",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "react-native-worklets-core": "1.6.2",
    "vision-camera-resize-plugin": "^3.2.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
